generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Site {
  id        String    @id @default(cuid())
  name      String
  location  String?
  client    String?
  startDate DateTime?
  endDate   DateTime?
  status    String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}

